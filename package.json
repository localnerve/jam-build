{
  "name": "jam-build",
  "version": "2.0.0",
  "description": "An adventurous, scalable, fullstack web application reference project",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run gulp-run -- build",
    "build:debug": "npm run gulp-debug -- build",
    "build:dev": "npm run lint && npm run gulp-run -- dev-build",
    "build:dev:debug": "npm run gulp-debug -- dev-build",
    "build:env": "cross-env AUTHZ_CLIENT_ID=${AUTHZ_CLIENT_ID:-E37D308D-9068-4FCC-BFFB-2AA535014B64} AUTHZ_URL=${AUTHZ_URL:-http://localhost:9010}",
    "dev": "node src/application/server --PORT=5000 --ENV-PATH=private/host-env.json",
    "dev:debug": "cross-env DEBUG=server*,api* node --inspect-brk src/application/server/index.js --PORT=5000 --ENV-PATH=private/host-env.json --DEBUG",
    "dev:cover": "c8 node src/application/server --PORT=5000 --ENV-PATH=private/host-env.json --TEST",
    "gulp-run": "npm run build:env -- node --no-experimental-require-module ./node_modules/gulp/bin/gulp.js",
    "gulp-debug": "npm run build:env -- node --no-experimental-require-module --inspect-brk ./node_modules/gulp/bin/gulp.js",
    "install-arm": "pushd node_modules/@localnerve/gulp-imagemin && npm run install-arm && popd",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.scss",
    "maint": "node src/application/server --MAINTENANCE=\"`date -v+2H -u +'%a, %d %b %Y %H:%M:%S GMT'`\" --ENV-PATH=private/host-env.json",
    "start": "NODE_ENV=production node src/application/server --ENV-PATH=private/host-env.json",
    "test": "npm run test:env -- playwright test --project=api --project=pages",
    "test:env": "cross-env AUTHZ_ADMIN_SECRET=deadbEf-2af536fa DB_DATABASE=jam_build DB_USER=jbuser DB_APP_USER=jbadmin DB_PASSWORD=deadbEef-5dccb117 DB_ROOT_PASSWORD=deadbEef-0b62360e DB_APP_PASSWORD=deadbEef-47148f2f",
    "test:env:local": "npm run test:env -- AUTHZ_URL=http://localhost:9010 AUTHZ_CLIENT_ID=E37D308D-9068-4FCC-BFFB-2AA535014B64 LOCALHOST_PORT=5000",
    "test:local": "npm run test:env:local -- playwright test --project=api --project=pages",
    "test:local:debug": "npm run test:env:local -- DEBUG=server*,api*,test* playwright test --project=api-debug --project=pages --workers=1 --timeout=600000 --headed",
    "test:build": "npm run test:env -- DEBUG=testcontainers*,server*,api*,test* FORCE_BUILD=1 playwright test --project=dummy --headed",
    "test:debug2": "npm run test:env -- DEBUG=server*,api*,test* playwright test --project=api --project=pages --workers=2 --timeout=600000 --headed",
    "test:debug": "npm run test:env -- DEBUG=server*,api*,test* playwright test --project=api --project=pages --workers=1 --timeout=600000 --headed",
    "test:_user": "node src/test/_createlocaluser.js",
    "test:_data": "npm run test:env:local -- DEBUG=api*,test* playwright test --project=localdata"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localnerve/jam-build.git"
  },
  "keywords": [
    "jam",
    "build",
    "fullstack",
    "web",
    "app",
    "development",
    "pwa",
    "scalable",
    "architecture"
  ],
  "author": "Alex Grant <alex@localnerve.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/localnerve/jam-build/issues"
  },
  "homepage": "https://github.com/localnerve/jam-build#readme",
  "dependencies": {
    "@localnerve/authorizer-js": "^1.0.1",
    "@localnerve/debug": "^1.0.4",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-version-request": "^1.7.0",
    "express-version-route": "^1.2.4",
    "mariadb": "^3.4.2",
    "pino": "^9.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@localnerve/csp-hashes": "^5.0.9",
    "@localnerve/fast-is-equal": "^1.0.1",
    "@localnerve/gulp-imagemin": "^11.6.0",
    "@localnerve/gulp-responsive": "^7.3.3",
    "@localnerve/sass-asset-functions": "^6.14.0",
    "@playwright/test": "^1.52.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@testcontainers/mariadb": "^11.0.0",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-compat": "^6.0.2",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "gulp": "^5.0.0",
    "gulp-filter": "^9.0.1",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rev": "^11.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-rewrite": "^6.0.0",
    "handlebars": "^4.7.8",
    "idb": "^8.0.3",
    "mkdirp": "^3.0.1",
    "modern-normalize": "^3.0.1",
    "pino-pretty": "^13.0.0",
    "plugin-error": "^2.0.1",
    "puppeteer": "^24.9.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "rollup-plugin-output-manifest": "^2.0.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-visualizer": "^6.0.1",
    "sass": "^1.89.1",
    "sitemap": "^8.0.0",
    "smob": "^1.5.0",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "tar": "^7.4.3",
    "testcontainers": "^11.0.0",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "download": "npm:@xhmikosr/downloader",
    "bin-wrapper": "npm:@xhmikosr/bin-wrapper",
    "bin-build": "npm:@localnerve/bin-build",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec",
    "got": "^13.0.0",
    "semver-regex": "^4.0.5",
    "cross-spawn": "^7.0.6",
    "@localnerve/csp-hashes": {
      "undici": "^6.21.3"
    }
  },
  "engines": {
    "node": "22.15.0"
  }
}
