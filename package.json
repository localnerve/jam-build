{
  "name": "jam-build",
  "version": "2.0.0",
  "description": "A simple file-based jamstack webapp builder",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run gulp-run -- build",
    "build:debug": "npm run gulp-debug -- build",
    "build:dev": "npm run lint && npm run gulp-run -- dev-build",
    "build:dev:debug": "npm run gulp-debug -- dev-build",
    "dev": "node src/application/server --PORT=5000 --ENV-PATH=private/host-env.json",
    "dev:debug": "node --inspect-brk src/application/server/index.js --PORT=5000 --ENV-PATH=private/host-env.json --DEBUG",
    "gulp-run": "node --no-experimental-require-module ./node_modules/gulp/bin/gulp.js",
    "gulp-debug": "node --no-experimental-require-module --inspect-brk ./node_modules/gulp/bin/gulp.js",
    "install-arm": "pushd node_modules/@localnerve/gulp-imagemin && npm run install-arm && popd",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint .",
    "lint:css": "stylelint src/**/*.scss",
    "maint": "node src/application/server --MAINTENANCE=\"`date -v+2H -u +'%a, %d %b %Y %H:%M:%S GMT'`\" --ENV-PATH=private/host-env.json",
    "start": "NODE_ENV=production node src/application/server --ENV-PATH=private/host-env.json",
    "test": "echo \"no tests yet\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localnerve/jam-build.git"
  },
  "keywords": [
    "jam",
    "build",
    "web",
    "app"
  ],
  "author": "Alex Grant <alex@localnerve.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/localnerve/jam-build/issues"
  },
  "homepage": "https://github.com/localnerve/jam-build#readme",
  "dependencies": {
    "compression": "^1.8.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@localnerve/csp-hashes": "^5.1.2",
    "@localnerve/gulp-imagemin": "^11.6.0",
    "@localnerve/gulp-responsive": "^7.4.2",
    "@localnerve/sass-asset-functions": "^7.0.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-compat": "^6.0.2",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "gulp-filter": "^9.0.1",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-postcss": "^10.0.0",
    "gulp-rev": "^11.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-rewrite": "^6.0.0",
    "handlebars": "^4.7.8",
    "mkdirp": "^3.0.1",
    "modern-normalize": "^3.0.1",
    "plugin-error": "^2.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-output-manifest": "^2.0.0",
    "sass": "^1.90.0",
    "sitemap": "^8.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "workbox-build": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "download": "npm:@xhmikosr/downloader",
    "bin-wrapper": "npm:@xhmikosr/bin-wrapper",
    "bin-build": "npm:@localnerve/bin-build",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec",
    "got": "^13.0.0",
    "semver-regex": "^4.0.5",
    "cross-spawn": "^7.0.6"
  }
}
